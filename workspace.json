{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "roma-api",
  "projects": {
    "angular-core": {
      "root": "libs/angular/core",
      "sourceRoot": "libs/angular/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular/core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/angular/core"
          ],
          "options": {
            "jestConfig": "libs/angular/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "angular-shared": {
      "root": "libs/angular/shared",
      "sourceRoot": "libs/angular/shared/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/angular/shared/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/angular/shared"
          ],
          "options": {
            "jestConfig": "libs/angular/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-core": {
      "root": "libs/api/core",
      "sourceRoot": "libs/api/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api/core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/api/core"
          ],
          "options": {
            "jestConfig": "libs/api/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-shared": {
      "root": "libs/api/shared",
      "sourceRoot": "libs/api/shared/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/api/shared/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/api/shared"
          ],
          "options": {
            "jestConfig": "libs/api/shared/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "madero": {
      "projectType": "library",
      "root": "libs/madero",
      "sourceRoot": "libs/madero/src",
      "prefix": "simples-empresa",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/madero"
          ],
          "options": {
            "jestConfig": "libs/madero/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/madero/src/**/*.ts",
              "libs/madero/src/**/*.html"
            ]
          }
        }
      }
    },
    "roma-api": {
      "root": "apps/roma-api",
      "sourceRoot": "apps/roma-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/roma-api",
            "main": "apps/roma-api/src/main.ts",
            "tsConfig": "apps/roma-api/tsconfig.app.json",
            "assets": [
              "apps/roma-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/roma-api/src/environments/environment.ts",
                  "with": "apps/roma-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "roma-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/roma-api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/roma-api"
          ],
          "options": {
            "jestConfig": "apps/roma-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "roma-app": {
      "projectType": "application",
      "root": "apps/roma-app",
      "sourceRoot": "apps/roma-app/src",
      "prefix": "simples-empresa",
      "implicitDependencies": [
        "shared-assets",
        "shared-styles"
      ],
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/roma-app",
            "index": "apps/roma-app/src/index.html",
            "main": "apps/roma-app/src/main.ts",
            "polyfills": "apps/roma-app/src/polyfills.ts",
            "tsConfig": "apps/roma-app/tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "libs/shared/assets/src",
                "output": "./"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/assets/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "apps/roma-app/src/styles.scss",
              "libs/shared/styles/src/index.scss",
              "libs/shared/styles/src/index.scss",
              "libs/madero/src/lib/style.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/shared/styles/src/",
                "libs/shared/styles/src/lib/",
                "libs/shared/styles/src/lib/",
                "libs/madero/src/lib/style"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "roma-app:build:production"
            },
            "development": {
              "browserTarget": "roma-app:build:development",
              "port": 5000
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "roma-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/roma-app/src/**/*.ts",
              "apps/roma-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/roma-app"
          ],
          "options": {
            "jestConfig": "apps/roma-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "roma-app-e2e": {
      "root": "apps/roma-app-e2e",
      "sourceRoot": "apps/roma-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/roma-app-e2e/cypress.json",
            "tsConfig": "apps/roma-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "roma-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "roma-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/roma-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-assets": {
      "projectType": "library",
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src"
    },
    "shared-environments": {
      "root": "libs/shared/environments",
      "sourceRoot": "libs/shared/environments/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/environments/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/environments"
          ],
          "options": {
            "jestConfig": "libs/shared/environments/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-styles": {
      "projectType": "library",
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src"
    }
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
